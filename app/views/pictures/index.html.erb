<p id="notice"><%= notice %></p>

</div>

<h1>Photogur Feed</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= link_to 'New Picture', new_picture_path %>
      <% @pictures.each do |picture| %>
        <tr>
          <td><%= image_tag picture.url, class: "img_resize"%></td>
        </tr>
      <% end %>
  </tbody>
</table>

<br>
